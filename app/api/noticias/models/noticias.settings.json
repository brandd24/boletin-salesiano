{
  "kind": "collectionType",
  "collectionName": "noticias",
  "info": {
    "name": "Noticias",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "summary": {
      "type": "richtext"
    },
    "cover_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "gallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "date": {
      "type": "date",
      "required": true
    },
    "country": {
      "type": "enumeration",
      "enum": [
        "El_Salvador",
        "Guatemala",
        "Honduras",
        "Nicaragua",
        "Costa_Rica",
        "Panama"
      ]
    },
    "content": {
      "type": "richtext"
    },
    "author_name": {
      "type": "string"
    },
    "tags": {
      "type": "json"
    },
    "categories": {
      "collection": "category",
      "via": "noticia"
    },
    "blogs": {
      "via": "noticias",
      "collection": "blogs"
    },
    "flickr_album_id": {
      "type": "string"
    }
  }
}
